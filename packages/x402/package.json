{
  "name": "stakefy-x402-components",
  "version": "0.1.0",
  "description": "React Paywall + SessionBudget components for Stakefy x402",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "default": "./dist/core/index.js"
    },
    "./express": {
      "types": "./dist/express/index.d.ts",
      "default": "./dist/express/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    }
  },
  "keywords": ["stakefy", "x402", "react", "paywall", "session-budget", "components"],
  "author": "stakefyx402",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/**/*.ts src/**/*.tsx --dts --format esm --clean --tsconfig tsconfig.build.json",
    "types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "test": "vitest"
  },
  "peerDependencies": {
    "express": ">=4",
    "react": ">=18"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "tsup": "^8.0.1",
    "typescript": "^5.6.3",
    "vitest": "^2.0.5"
  }
}
