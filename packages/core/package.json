{
  "name": "x402-stakefy-sdk",
  "version": "2.1.1",
  "description": "Complete Solana x402 payment infrastructure - Core SDK + React Hooks + Express Middleware",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react --external express --external @solana/wallet-adapter-react --external @solana/web3.js",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external react --external express --external @solana/wallet-adapter-react --external @solana/web3.js --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "solana",
    "x402",
    "payments",
    "stakefy",
    "web3",
    "cryptocurrency",
    "react",
    "hooks",
    "express",
    "middleware",
    "api",
    "paywall"
  ],
  "author": "stakefyx402",
  "license": "MIT",
  "homepage": "https://github.com/JaspSoe/stakefy-x402#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaspSoe/stakefy-x402.git",
    "directory": "packages/core"
  },
  "peerDependencies": {
    "@solana/wallet-adapter-react": "^0.15.0",
    "@solana/web3.js": "^1.95.0",
    "express": "^4.18.0 || ^5.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "@solana/wallet-adapter-react": {
      "optional": true
    },
    "@solana/web3.js": {
      "optional": true
    }
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.14",
    "x402-stakefy-sdk": "^2.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/web3.js": "^1.95.8",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "express": "^4.19.2",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
