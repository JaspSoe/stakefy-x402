{
  "name": "x402-stakefy-sdk",
  "version": "3.0.1",
  "description": "The most feature-rich x402 payment SDK for Solana - 10x lower fees (0.1%), social payments, session budgets, payment channels, React hooks, and Express middleware",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react --external express --external @solana/wallet-adapter-react --external @solana/web3.js",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external react --external express --external @solana/wallet-adapter-react --external @solana/web3.js --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "x402",
    "solana",
    "payments",
    "stakefy",
    "web3",
    "crypto",
    "usdc",
    "stablecoin",
    "payment-sdk",
    "solana-pay",
    "react-hooks",
    "express-middleware",
    "payment-gateway",
    "micropayments",
    "blockchain",
    "defi",
    "paywall",
    "subscription",
    "recurring-payments",
    "session-budget",
    "payment-channel",
    "social-payments",
    "ai-payments",
    "agent-payments",
    "402-protocol",
    "http-402",
    "pay-per-use",
    "api-monetization",
    "typescript"
  ],
  "author": "Stakefy Team <sayhello@stakefy.io>",
  "license": "MIT",
  "homepage": "https://github.com/JaspSoe/stakefy-x402#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaspSoe/stakefy-x402.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/JaspSoe/stakefy-x402/issues",
    "email": "sayhello@stakefy.io"
  },
  "peerDependencies": {
    "@solana/wallet-adapter-react": "^0.15.0",
    "@solana/web3.js": "^1.95.0",
    "express": "^4.18.0 || ^5.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "@solana/wallet-adapter-react": {
      "optional": true
    },
    "@solana/web3.js": {
      "optional": true
    }
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.14",
