    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/web3.js": "^1.95.8",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "express": "^4.19.2",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
