    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "x402",
    "express",
    "middleware",
    "solana",
    "payments",
    "stakefy",
    "paywall",
    "api",
    "web3",
    "crypto",
    "usdc",
    "api-monetization",
    "session-budget",
    "payment-gateway",
    "typescript"
  ],
  "author": "Stakefy Team <sayhello@stakefy.io>",
  "license": "MIT",
  "homepage": "https://github.com/JaspSoe/stakefy-x402#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaspSoe/stakefy-x402.git",
    "directory": "packages/express"
  },
  "bugs": {
    "url": "https://github.com/JaspSoe/stakefy-x402/issues",
    "email": "sayhello@stakefy.io"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  },
  "dependencies": {
    "x402-stakefy-sdk": "^2.4.0"
  },
