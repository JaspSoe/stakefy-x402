{
  "name": "stakefy-express",
  "version": "2.0.0",
  "description": "Express middleware for Stakefy x402 - Drop-in paywalls with 10x lower fees than competitors",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external express",
