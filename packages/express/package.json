{
  "name": "stakefy-express",
  "version": "1.0.2",
  "description": "Express middleware for Stakefy x402 payments - Add paywalls in 3 lines",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external express",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external express --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "express",
    "middleware",
    "solana",
    "x402",
    "payments",
    "stakefy",
    "paywall",
    "api"
  ],
  "author": "stakefyx402",
  "license": "MIT",
  "homepage": "https://github.com/JaspSoe/stakefy-x402#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaspSoe/stakefy-x402.git",
    "directory": "packages/express"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  },
  "dependencies": {
    "x402-stakefy-sdk": "^1.0.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.1",
    "express": "^4.19.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
