{
  "name": "x402-stakefy-react",
  "version": "2.0.0",
  "description": "React hooks and components for Stakefy x402 - 10x lower fees than competitors, social payments, session budgets",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.tsx --format cjs,esm --dts --external react --external @solana/wallet-adapter-react --external @solana/web3.js",
    "dev": "tsup src/index.tsx --format cjs,esm --dts --external react --external @solana/wallet-adapter-react --external @solana/web3.js --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "x402",
    "react",
    "hooks",
    "solana",
    "payments",
    "stakefy",
    "web3",
    "crypto",
    "usdc",
    "payment-sdk",
    "react-hooks",
    "components",
    "paywall",
    "session-budget",
    "social-payments",
    "typescript"
  ],
  "author": "Stakefy Team <sayhello@stakefy.io>",
  "license": "MIT",
  "homepage": "https://github.com/JaspSoe/stakefy-x402#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaspSoe/stakefy-x402.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/JaspSoe/stakefy-x402/issues",
    "email": "sayhello@stakefy.io"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "@solana/wallet-adapter-react": "^0.15.0",
    "@solana/web3.js": "^1.95.0"
  },
  "dependencies": {
    "x402-stakefy-sdk": "^2.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/node": "^22.10.1",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/web3.js": "^1.95.8",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
